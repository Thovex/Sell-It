<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="nl.hku.studenthome.sell_it">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="SolliciTraining"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainMenu"
            android:screenOrientation="landscape" >

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="MainActivity"
            android:screenOrientation="landscape"/>
        <activity
            android:name=".SettingsActivity"
            android:label="Settings"
            android:screenOrientation="landscape"/>
        <activity
            android:name=".SectorSelect"
            android:label="Sector Select"
            android:screenOrientation="landscape"/>
        <activity
            android:name=".SectorItem"
            android:screenOrientation="landscape"/>
        <activity
            android:name=".SollicitatieGesprek"
            android:label="Sollicitatie Gesprek"
            android:screenOrientation="landscape"/>
        <activity
            android:name=".PortfolioCheck"
            android:label="Portfolio Check" />
        <activity
            android:name=".ResultsActivity"
            android:screenOrientation="landscape" />

    </application>

</manifest>